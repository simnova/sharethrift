{
  "name": "@sthrift/ui-sharethrift-components",
  "version": "1.0.0",
  "description": "Reusable UI components (molecules, organisms, theme) for ShareThrift portals.",
  "type": "module",
  "main": "dist/src/index.js",
  "types": "src/index.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc --build && npm run copy-css",
    "copy-css": "cpx \"src/**/*.{css,svg,png,jpg,jpeg,gif}\" dist/src",
    "lint": "biome lint .",
    "test": "echo 'No tests yet'",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "peerDependencies": {
    "react": ">=17.0.0",
    "react-dom": ">=17.0.0"
