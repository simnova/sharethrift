    "@types/react": "^19.1.10",
    "@types/react-dom": "^19.1.7",
    "cpx": "^1.5.0",
    "storybook": "^9.1.2",
    "typescript": "^5.0.0"
  },
  "license": "MIT",
  "exports": {
    ".": {
      "import": "./dist/src/index.js",
