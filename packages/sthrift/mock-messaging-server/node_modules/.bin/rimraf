#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/kishorgupta/oneplace/src/sharethrift/node_modules/.pnpm/rimraf@6.0.1/node_modules/rimraf/dist/esm/node_modules:/Users/kishorgupta/oneplace/src/sharethrift/node_modules/.pnpm/rimraf@6.0.1/node_modules/rimraf/dist/node_modules:/Users/kishorgupta/oneplace/src/sharethrift/node_modules/.pnpm/rimraf@6.0.1/node_modules/rimraf/node_modules:/Users/kishorgupta/oneplace/src/sharethrift/node_modules/.pnpm/rimraf@6.0.1/node_modules:/Users/kishorgupta/oneplace/src/sharethrift/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/kishorgupta/oneplace/src/sharethrift/node_modules/.pnpm/rimraf@6.0.1/node_modules/rimraf/dist/esm/node_modules:/Users/kishorgupta/oneplace/src/sharethrift/node_modules/.pnpm/rimraf@6.0.1/node_modules/rimraf/dist/node_modules:/Users/kishorgupta/oneplace/src/sharethrift/node_modules/.pnpm/rimraf@6.0.1/node_modules/rimraf/node_modules:/Users/kishorgupta/oneplace/src/sharethrift/node_modules/.pnpm/rimraf@6.0.1/node_modules:/Users/kishorgupta/oneplace/src/sharethrift/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../rimraf/dist/esm/bin.mjs" "$@"
else
  exec node  "$basedir/../rimraf/dist/esm/bin.mjs" "$@"
fi
